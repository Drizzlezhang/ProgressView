<?xml version="1.0" encoding="utf-8"?>
<resources>

	<attr name="progress_text"
		format="string"/>
	<attr name="progress_text_size"
		format="dimension"/>
	<attr name="progress_text_color"
		format="color"/>
	<attr name="progress_radius"
		format="dimension"/>
	<attr name="progress_stroke_width"
		format="dimension"/>
	<attr name="progress_stroke_color"
		format="color"/>
	<attr name="progress_back_color"
		format="color"/>
	<attr name="need_back_circle"
		format="boolean"/>
	<attr name="need_back_color"
		format="boolean"/>
	<attr name="back_color"
		format="color"/>
	<attr name="is_touch_slide"
		format="boolean"/>
	<attr name="start_location">
		<flag name="top"
			value="1"/>
		<flag name="bottom"
			value="2"/>
		<flag name="left"
			value="3"/>
		<flag name="right"
			value="4"/>
	</attr>


	<declare-styleable name="ProgressView">
		<attr name="progress_text"/>
		<attr name="progress_text_size"/>
		<attr name="progress_text_color"/>
		<attr name="progress_radius"/>
		<attr name="progress_stroke_width"/>
		<attr name="progress_stroke_color"/>
		<attr name="progress_back_color"/>
		<attr name="need_back_circle"/>
		<attr name="need_back_color"/>
		<attr name="back_color"/>
		<attr name="start_location"/>
		<attr name="is_touch_slide"/>
	</declare-styleable>
</resources>